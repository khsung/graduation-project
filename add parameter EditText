//login_page

public class LoginPage extends AppCompatActivity {
    EditText et_id, et_pw;
    String s_id,s_pw;
    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_login_page);
        et_id = (EditText) findViewById(R.id.editText);
        et_pw = (EditText) findViewById(R.id.editText2);
    }
    public void ConfirmBtn(View v){
        s_id = et_id.getText().toString();
        s_pw = et_pw.getText().toString();
        finish();
        Intent intent = new Intent(getApplicationContext(), MainPage.class);
        startActivity(intent);
    }
}


//Membership_page

public class MembershipPage extends AppCompatActivity {
    EditText et_id, et_pw, et_name, et_resitnum, et_email;
    String s_id,s_pw, s_resitnum;
    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_membership_page);
        et_id = (EditText) findViewById(R.id.editText5);
        et_pw = (EditText) findViewById(R.id.editText10);
        et_name = (EditText) findViewById(R.id.editText8);
        et_resitnum = (EditText) findViewById(R.id.editText11);
        et_email = (EditText) findViewById(R.id.editText9);

    }
    public void ConfirmBtn(View v){
        s_id = et_id.getText().toString();
        s_pw = et_pw.getText().toString();
        s_resitnum = et_resitnum.getText().toString();
        finish();
    }
}
